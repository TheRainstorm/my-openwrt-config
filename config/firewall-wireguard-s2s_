config zone
        option name 'wg_s2s'    #zone name
        option input 'ACCEPT'
        option output 'ACCEPT'
        option forward 'ACCEPT'
        option masq '1'
        list network 'wg_s2s_b' #interface name

config forwarding
        option src 'wg_s2s'
        option dest 'lan'

config forwarding
        option src 'wg_s2s'
        option dest 'wan'

config forwarding
        option src 'lan'
        option dest 'wg_s2s'